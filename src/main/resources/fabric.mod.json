{
	"schemaVersion": 1,
	"id": "cobblemon-things",
	"version": "${version}",
	"name": "Cobblemon Things",
	"description": "Some Cobblemon things",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://cobblemonthings.cafezinhu.com/",
		"sources": "https://github.com/Cafezinhu/cobblemon-things"
	},
	"license": "CC0-1.0",
	"icon": "assets/cobblemon-things/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.cafezinhu.cobblemonthings.CobblemonThings",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.cafezinhu.cobblemonthings.CobblemonThingsDataGenerator",
				"adapter": "kotlin"
			}
		],
		"client": [
			"com.cafezinhu.cobblemonthings.CobblemonThingsClient"
		]
	},
	"mixins": [
		"cobblemon-things.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}